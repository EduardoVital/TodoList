etters } from 'vuex';\r\n\r\nexport default {\r\n  name: 'Header',\r\n  data() {\r\n    return {\r\n      task: '',\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters(['getToggle']),\r\n  },\r\n  methods: {\r\n    ...mapActions(['setTodoList']),\r\n    handleCreateNewTask() {\r\n      const randomId = Math.floor(Math.random() * 1000);\r\n      const newTask = {\r\n        id: randomId,\r\n        title: this.task,\r\n        isCompleted: false,\r\n      };\r\n\r\n      if (!this.task) {\r\n        return;\r\n      }\r\n\r\n      this.setTodoList(newTask);\r\n\r\n      this.task = '';\r\n      this.isActive = false;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.bg-tasklit-sun {\r\n  background: var(--background-tasklist-sun);\r\n}\r\n.bg-tasklit-moon {\r\n  background: var(--background-tasklist-moon);\r\n}\r\n.create-task {\r\n  max-width: 484px;\r\n  margin: 0 auto;\r\n  width: 100%;\r\n  padding: 15px 20px;\r\n  border-radius: 5px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: flex-start;\r\n\r\n  .input-text {\r\n    width: 90%;\r\n    outline: none;\r\n    border: none;\r\n    font-size: 16px;\r\n    font: 16px 'Josefin Sans', sans-serif;\r\n    color: var(--gray-placeholder);\r\n    background: transparent;\r\n  }\r\n}\r\n\r\n// media query\r\n@media (max-width: 720px) {\r\n  .create-task {\r\n    max-width: 320px;\r\n    padding: 10px;\r\n  }\r\n}\r\n@media (max-width: 320px) {\r\n  .create-task {\r\n    max-width: 300px;\r\n  }\r\n}\r\n</style>\r\n"]}]}